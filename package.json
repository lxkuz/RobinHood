  },
  "homepage": "https://github.com/lxkuz/RobinHood#readme",
  "dependencies": {
    "body-parser": "^1.15.0",
    "casperjs": "^1.1.0-beta5",
    "config": "^1.19.0",
    "express": "^4.13.4",
    "jade": "^1.11.0",
    "locallydb": "0.0.9",
    "mongoose": "^4.4.7",
    "node-telegram-bot-api": "^0.21.1",
    "npid": "^0.4.0",
    "require-reload": "^0.2.2",
    "sequelize": "^3.21.0",
    "spooky": "^0.2.5",
